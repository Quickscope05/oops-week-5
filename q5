#include <iostream>
using namespace std;
class Degree
{
public:
void getDegree()
{
cout<<"I got a degree"<<endl;
}
};
class PostGraduate : public Degree
{
public :
void getDegree()
{
cout<<"I am a Postgraduate"<<endl;
}
};
class UnderGraduate : public Degree
{
public :
void getDegree()
{
cout<<"I am a Undergraduate"<<endl;
}
};
int main()
{
Degree d;UnderGraduate u;PostGraduate p;
d.getDegree();
u.getDegree();
p.getDegree();
return 0;
}
