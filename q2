#include <iostream>
using namespace std;
class Student
{
public : string name;
Student()
{
name="Unknown";
}
Student(string n)
{
name=n;
}
void display()
{
cout<<"Name : "<<name<<endl;
}
};
int main()
{
Student s1;//without input
s1.display();
string n;cin>>n;
Student s2(n);//with input
s2.display();
return 0;
}
